{
  "root": true,
  "extends": [
    "airbnb",
    "prettier",
    "prettier/react"
  ],
  "plugins": [
    "import",
    "react",
    "prettier"
  ],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2016,
    "sourceType": "module",
    "jsx": true,
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "browser": true,
    "commonjs": true,
    "node": true,
    "es6": true,
    "jest": true,
    "mocha": true
  },
  "rules": {
    "no-debugger": "warn",
    "no-console": "warn",
    "import/prefer-default-export": "off",
    "react/jsx-filename-extension": "off",
    "react/jsx-no-bind": "off",
    "no-underscore-dangle": "off",
    "no-nested-ternary": "off",
    "jsx-a11y/click-events-have-key-events": "off",
    "react/prefer-stateless-function": "off",
    "jsx-a11y/no-autofocus": "off",
    "consistent-return": "off",
    "react/no-array-index-key": "off",
    "eqeqeq": "off",
    "no-multi-assign": "off",
    "import/no-mutable-exports": "off",
    "prefer-const": [
      "error",
      {
        "destructuring": "all"
      }
    ],
    "no-unused-expressions": [
      "error",
      {
        "allowShortCircuit": true
      }
    ],
    "linebreak-style": "off",
    "react/prop-types": "off",
    "react/jsx-sort-props": [
      "error",
      {
        "callbacksLast": true,
        "shorthandFirst": true,
        "noSortAlphabetically": false,
        "reservedFirst": true
      }
    ],
    "react/sort-comp": [
      "error",
      {
        "order": [
          "type-annotations",
          "static-methods",
          "lifecycle",
          "everything-else",
          "render"
        ]
      }
    ],
    "prettier/prettier": [
      "error",
      {
        "semi": true,
        "trailingComma": "es5",
        "useTabs": false,
        "printWidth": 80,
        "tabWidth": 2,
        "singleQuote": false,
        "bracketSpacing": false,
        "jsxBracketSameLine": false
      }
    ]
  }
}